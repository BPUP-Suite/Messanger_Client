{
  "expo": {
    "name": "BPUP Messanger",
    "slug": "Messanger",
    "version": "0.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/bpup_logo.png",
    "scheme": "myapp",
    "owner": "bpup",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.BPUP.Messanger"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bpup_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.BPUP.Messanger"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/bpup_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bpup_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6f29bfad-6db3-491f-9bbf-d97664dac861"
      }
    }
  }
}
